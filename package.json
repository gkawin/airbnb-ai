    "eslint": "^9.6.0",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": "20"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@firebase/app-compat",
        "@firebase/app-types"
      ]
    }
  }
}
